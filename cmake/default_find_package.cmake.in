set(@_DEPENDENCY@_INSTALL_DIR @_PACKAGE_INSTALL_DIR@)
set(@_DEPENDENCY@_CMAKE_PATH @_PACKAGE_INSTALL_DIR@/@_PACKAGE_CMAKE_RPATH@)

if (NOT @_DEPENDENCY@_REQUIRED)
    set(@_DEPENDENCY@_REQUIRED TRUE)
endif()

set(_@_DEPENDENCY@_FIND_CMAKE_ARGS CONFIG PATHS ${@_DEPENDENCY@_CMAKE_PATH} NO_DEFAULT_PATH)

message(INFO " -- [Superbuild] Looking for package @_PACKAGE_NAME@")
if (@_DEPENDENCY@_REQUIRED)
    find_package(@_PACKAGE_NAME@ REQUIRED ${_@_DEPENDENCY@_FIND_CMAKE_ARGS})
else()
    find_package(@_PACKAGE_NAME@ ${_@_DEPENDENCY@_FIND_CMAKE_ARGS})
endif()